{
  "name": "marvin",
  "version": "0.0.0",
  "description": "An API driven skin for MediaWiki",
  "scripts": {
    "start": "run-p -n dev:*",
    "dev:client": "NODE_ENV=development webpack -w",
    "dev:server": "NODE_ENV=development nodemon -w dist/client -w src/server/ src/server/index.js",
    "prod:build": "NODE_ENV=production webpack -p",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niedzielski/marvin.git"
  },
  "keywords": [
    "mediawiki",
    "wikimedia",
    "web",
    "node",
    "pwa"
  ],
  "author": "Wikimedia Foundation",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/project/profile/2878/"
  },
  "homepage": "https://phabricator.wikimedia.org/project/profile/2878/",
  "dependencies": {
    "express": "^4.15.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^3.19.0",
    "eslint-config-node-services": "^2.2.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-wikimedia": "^0.4.0",
    "eslint-plugin-jsdoc": "^3.1.2",
    "eslint-plugin-json": "^1.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "webpack": "^3.4.1"
  }
}
