{
  "name": "marvin",
  "version": "0.0.0",
  "description": "An API driven skin for MediaWiki",
  "scripts": {
    "start": "run-p -sn dev:\\*",
    "dev:client": "NODE_ENV=development webpack -w",
    "dev:server": "NODE_ENV=development nodemon -q src/server/index.js",
    "prod:build": "NODE_ENV=production webpack -p",
    "format": "npm run lint -s -- --fix",
    "format:all": "npm run format -s -- .",
    "lint": "eslint --cache --max-warnings 0 --ext js,json",
    "lint:all": "npm run lint -s -- .",
    "pretest": "npm run lint:all -s",
    "test": "mocha '{src,test}/**/*.test.js'",
    "test:watch": "nodemon -q -x 'npm test'",
    "precommit": "npm run test -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://phabricator.wikimedia.org/source/marvin.git"
  },
  "keywords": [
    "mediawiki",
    "wikimedia",
    "web",
    "node",
    "pwa"
  ],
  "author": "Wikimedia Foundation",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/project/profile/2878/"
  },
  "homepage": "https://phabricator.wikimedia.org/project/profile/2878/",
  "engines": {
    "node": ">= 6",
    "npm": ">= 5"
  },
  "dependencies": {
    "express": "^4.15.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^3.19.0",
    "eslint-config-node-services": "^2.2.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-wikimedia": "^0.4.0",
    "eslint-plugin-jsdoc": "^3.1.2",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "sinon": "^3.0.0",
    "webpack": "^3.4.1"
  }
}
