{
  "extends": [
    "node-services",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {"parser": "typescript"}]
  },
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true
    }
  },
  "env": {
    "es6": true,
    "node": false
  }
}
